<div class="container mt-3">
    <h5>Total Results Ready To Be Delivered To A Facility</h5>
    <div class="row">
        <div class="col-12">
            <table id="results_ready_at_molecular_table" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Facility To Deliver Results To</th>
                    <th>Total Results Ready</th>
                </tr>
                </thead>
            </table>
        </div>
    </div>
</div>

<script>
