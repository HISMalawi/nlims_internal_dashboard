    //                 {'facility': 'Kawale health centre', 'results_ready_at_molecular': 356, 'target_lab': 'Lilongwe'},
    //                 {'facility': 'St Gabriel Hospital', 'results_ready_at_molecular': 307, 'target_lab': 'Lilongwe'}
    //             ];
    
    $('#results_ready_at_molecular_table').DataTable( {
                data: dataset,
                columns: [
                    { data: 'facility' },
                    { data: 'results_ready_at_molecular' }
                ],
        } );
});
</script>